int cmp s1, s2:
if s1.len != s2.len:
	return 0
for i in 0..s1.len:
	for j in (i + 1)..s1.len:
		if s1[i][0] == s2[j][0]:
			if s1[i][0] == 0:
				if s1[i][1] == s2[j][1]:
					goto FOUND
			else:
				if cmp s1[i][1], s2[j][1]:
					goto FOUND
	return 0
	FOUND:
return 1

int in is_set, v, s:
let t0 = s.len
if is_set == 1:
	for i in 0..t0:
		if s[i][0] == 1:
			if set_cmp s[i][1], v
				return 1
else:
	for i in 0..t0:
		if s[i][0] == 0:
			if s[i][1] == v:
				return 1
return 0
